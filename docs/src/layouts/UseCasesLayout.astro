---
import BaseLayout from "@layouts/BaseLayout.astro";
import BoostedScript from "@components/BoostedScript.astro";
import { getConfig } from '@libs/config'

interface Props {
  data: {
    title: string
  }
}

const { data } = Astro.props

const version = getConfig().docs_version
const echartsVersion = getConfig().echarts_version
---

<BaseLayout data={data} removeBoosted={true}>
  <BoostedScript />
  <script slot="head-scripts" src={`https://cdn.jsdelivr.net/npm/echarts@${echartsVersion}/dist/echarts.min.js`} crossorigin="anonymous"></script>
  <script slot="head-scripts" src={`/${version}/use-cases.js`}></script>
  <slot />
</BaseLayout>