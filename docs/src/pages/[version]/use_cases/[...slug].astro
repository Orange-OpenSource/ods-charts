---
import UseCasesLayout from '@layouts/UseCasesLayout.astro';
import { useCasesPages } from '@libs/content'

export async function getStaticPaths() {
  return useCasesPages.map((docsPage) => {
    return {
      params: { slug: docsPage.slug, version: '0.3' },
      props: docsPage
    }
  })
}

const { data, render } = Astro.props
const { Content } = await render()
---

<UseCasesLayout data={data}>
  <Content />
</UseCasesLayout>
