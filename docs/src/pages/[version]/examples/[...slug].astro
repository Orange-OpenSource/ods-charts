---
import ExamplesLayout from '@layouts/ExamplesLayout.astro';
import { examplesPages } from '@libs/content'

export async function getStaticPaths() {
  return examplesPages.map((docsPage) => {
    return {
      params: { slug: docsPage.slug, version: '0.3' },
      props: docsPage
    }
  })
}

const { data, render } = Astro.props
const { Content } = await render()
---

<ExamplesLayout data={data}>
  <Content />
</ExamplesLayout>
